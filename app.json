{
  "expo": {
    "name": "Smartstore Mobile",
    "slug": "smartstore-mobile-revision",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mojila.smartstore"
    },
    "android":{
      "package": "com.mojila.smartstore"
    },
    "description": "Aplikasi ini digunakan untuk pencarian, pemesanan, dan rencana pengambilan material pada gudang Smartstore"
  }
}
